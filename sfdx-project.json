{
  "packageDirectories": [
    {
      "path": "./src-env-specific-alias-pre",
      "package": "src-env-specific-alias-pre",
      "versionNumber": "1.0.0.0",
      "aliasfy": true,
      "ignoreOnStage": ["prepare", "validate", "quickbuild", "build"]
    },
    {
      "path": "src/frameworks/trigger-framework",
      "package": "trigger-framework",
      "versionNumber": "1.0.2.0",
      "versionName": "trigger-framework-1.0"
    },
    {
      "path": "src/frameworks/trigger-bypass-strategy",
      "package": "trigger-bypass-strategy",
      "versionNumber": "1.0.2.0",
      "versionName": "trigger-bypass-strategy-1.0",
      "dependencies": [
        {
          "package": "trigger-framework",
          "versionNumber": "1.0.0.LATEST"
        }
      ]
    },
    {
      "path": "./src/core-crm",
      "package": "core",
      "default": true,
      "versionName": "Version 1.0.0.0",
      "versionNumber": "1.0.0.0"
    }
  ],
  "namespace": "",
  "sfdcLoginUrl": "https://login.salesforce.com",
  "sourceApiVersion": "52.0",
  "packageAliases": {
    "DocuSign Apps Launcher": "04t3u00000349bDAAQ"
  },
  "plugins": {
    "sfpowerscripts": {
      "ignoreFiles": {
        "validate": ".forceignores/.buildignore",
        "build": ".forceignores/.validationignore"
      }
    },
    "sfdx-plugin-prettier": {
      "enabled": false
    }
  }
}
