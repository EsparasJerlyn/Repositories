    <sharingCriteriaRules>
        <fullName>ACP_Alumni_Engagement_Share</fullName>
        <accessLevel>Read</accessLevel>
        <description>Share Corporate Partnership Opportunity records to Alumni Engagement team with Read access</description>
        <label>ACP - Alumni Engagement Share</label>
        <sharedTo>
            <roleAndSubordinatesInternal>Alumni_Engagement</roleAndSubordinatesInternal>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Corporate Partnership</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
