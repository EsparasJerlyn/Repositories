<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Future_Students_Domestic_Share</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Share Inbound Enquiry Cases to Future Students Domestic with Read/Write access.</description>
        <label>Future Students Domestic Share</label>
        <sharedTo>
            <group>Future_Students_Domestic</group>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Inbound Enquiry</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
