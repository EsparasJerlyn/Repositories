<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>ADV_Donor_Engagement_Manager_Share_Parent_Stewardship</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Share Document records to Donor Engagement Manager when under Stewardship</description>
        <label>ADV Donor Engagement Manager Share - Parent Stewardship</label>
        <sharedTo>
            <group>Donor_Engagement_Manager</group>
        </sharedTo>
        <criteriaItems>
            <field>Stewardship__c</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>All_Advancement_Share_Parent_Opportunity</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Share Document records to all Advancement persona when under Opportunity</description>
        <label>All Advancement Share - Parent Opportunity</label>
        <sharedTo>
            <group>Advancement</group>
        </sharedTo>
        <criteriaItems>
            <field>Opportunity__c</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>IE_Operations_Manager_Share</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Share Document records to Operations Manager with Read/Write access when under
            Opportunity Outcome.</description>
        <label>IE Operations Manager Share</label>
        <sharedTo>
            <role>Operations_Manager</role>
        </sharedTo>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>Opportunity_Outcome__c</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <criteriaItems>
            <field>IP_Management__c</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>IE_Partnership_Manager_Share</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Share Document records to Partnership Manager with Read/Write access when under
            Engagement or Opportunity.</description>
        <label>IE Partnership Manager Share</label>
        <sharedTo>
            <role>Partnership_Manager</role>
        </sharedTo>
        <booleanFilter>1 OR 2 OR 3</booleanFilter>
        <criteriaItems>
            <field>Engagement__c</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity__c</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <criteriaItems>
            <field>IP_Management__c</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
</SharingRules>
