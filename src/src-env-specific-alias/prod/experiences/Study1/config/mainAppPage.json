/integrations/QUT-2017-Javascript-Folder.min.js\"></script>\n<script src=\"https://www.qut.edu.au/study/professional-and-executive-education/configuration/integrations/qutex-javascript.js\"></script>\n\n\n<script>\n    \nQUT.Console.enable();\n\nvar initHeader = function(){\n    console.log(\"document ready\");\n    // Set the call button for QUTex\n    $('.header__links__contact').attr(\"href\",\"#\");\n    $('.header__links__contact').on('click', function(){\n        if($(window).width() < 767){\n            // perform call\n            window.location.href = \"tel: +61 7 3138 7733\"\n        }else{\n            // Link to Contact Page\n            window.location.href = \"https://www.qut.edu.au/about/contact\";\n        }});\n\n    QUT.Megamenu.init();\n    console.log(\"mega init\");\n\n    $(\".header .megamenu__overlay\").css(\"top\",\"205px !important\");\n\n    function toggleFatFooterTab(e) {\n        console.log(\"fat footer pressed\");\n    if ($('.fatfooter__content__wrapper').is(':visible')) {\n        e.preventDefault();\n        \n        if (!$(this).hasClass('fatfooter__tab--current')) {\n            $(\".fatfooter__tab--current, .fatfooter__content--current\").removeClass('fatfooter__tab--current fatfooter__content--current');\n            $(this).addClass('fatfooter__tab--current');\n            $('#' + $(this).data('tab')).addClass('fatfooter__content--current');\n        }\n    }\n    }\n\n    $(\".fatfooter__tab\").click(function(e) {\n        toggleFatFooterTab.call(this, e);\n    });\n    \n    let nav = \"#secondary-qutex-nav\";\n    $(nav + \" .qutex-nav-button\").click(function(){\n        $(nav + \" ul\").slideToggle();\n        $(this).toggleClass(\"open\")\n        let ariaExpand = $(this).attr(\"aria-expanded\")\n        $(this).attr(\"aria-expanded\", ariaExpand == \"true\" ? \"false\" : \"true\");\n    });\n}\n    \n\nlet observer = new MutationObserver((mutations) => {\n  mutations.forEach((mutation) => {\n    if (mutation.addedNodes.length == 0) return\n\n    for (let i = 0; i < mutation.addedNodes.length; i++) {\n      let node = mutation.addedNodes[i]\n        if(mutation.addedNodes[i].children != undefined && mutation.addedNodes[i].children.length > 0){\n            var childrenList = mutation.addedNodes[i].children;\n            for(let x = 0; x < childrenList.length; x++) {\n                if(childrenList[x] != undefined && childrenList[x].className == \"qutex-courses qutex-update-2021   animated-page\"){\n                    setTimeout(function(){\n                        initHeader();\n                    },250);\n                    observer.disconnect(); \n                }\n            }\n        }\n    }\n  })\n})\n\n$(document).ready(function(){\n    observer.observe(document.body, {\n        childList: true\n      , subtree: true\n      , attributes: false\n      , characterData: false\n    })\n    setTimeout(function(){\n        // stop watching using:\n        observer.disconnect()\n    },10000)\n})\n\n</script>\n\n<!-- Global site tag (gtag.js) - Google Analytics -->\n<script async src=https://www.googletagmanager.com/gtag/js?id=G-GS4HMP12MP></script>\n<script>\n  window.dataLayer = window.dataLayer || [];\n  function gtag(){dataLayer.push(arguments);}\n  gtag('js', new Date());\n\n  gtag('config', 'G-GS4HMP12MP');\n</script>\n\n<!-- Google Tag Manager -->\n<script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':\nnew Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],\nj=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=\n'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);\n})(window,document,'script','dataLayer','GTM-WQ4HLR9');</script>\n<!-- End Google Tag Manager -->\n\n<!-- Google Tag Manager (noscript) -->\n<noscript><iframe src=ns \nheight=\"0\" width=\"0\" style=\"display:none;visibility:hidden\"></iframe></noscript>\n<!-- End Google Tag Manager (noscript) -->\n\n<!-- Meta Pixel Code -->\n<scr