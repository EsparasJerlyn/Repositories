    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Update_CLMS_Contract_Status_to_Negotiate</targetReference>
        </connector>
        <filterFormula>AND(
{!$Record.RecordType.DeveloperName} = &apos;Corporate_Partnership&apos;,
AND(
ISBLANK({!$Record__Prior.CLMS_Contract_ID__c}),
NOT(ISBLANK({!$Record.CLMS_Contract_ID__c}))
),
ISPICKVAL({!$Record.CLMS_Contract_Status__c}, &apos;&apos;)
)</filterFormula>
        <object>Opportunity</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
