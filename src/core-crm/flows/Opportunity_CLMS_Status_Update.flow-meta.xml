    <recordUpdates>
        <name>Update_CLMS_Contract_Status_to_Negotiate</name>
        <label>Update CLMS Contract Status to Negotiate</label>
        <locationX>176</locationX>
        <locationY>287</locationY>
        <inputAssignments>
            <field>CLMS_Contract_Status__c</field>
            <value>
                <stringValue>Negotiation</stringValue>
