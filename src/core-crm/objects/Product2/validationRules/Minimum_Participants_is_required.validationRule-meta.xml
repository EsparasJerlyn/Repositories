<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Minimum_Participants_is_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 Is_Group_Registration_Available__c = true,
 ISBLANK( Minimum_Participants_Group__c ) 
)</errorConditionFormula>
    <errorDisplayField>Minimum_Participants_Group__c</errorDisplayField>
    <errorMessage>Minimum Participants (Group) is required</errorMessage>
</ValidationRule>
