<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Predefined_Amount_4_Ascending_Order</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISNULL(Donation_Cause__c)),
Predefined_Amount_4__c &lt; Predefined_Amount_3__c
)</errorConditionFormula>
    <errorDisplayField>Predefined_Amount_4__c</errorDisplayField>
    <errorMessage>The predefined amount values should be in ascending order.</errorMessage>
</ValidationRule>
