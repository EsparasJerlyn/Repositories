<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Staff_Activation_End_Date</fullName>
    <active>true</active>
    <description>Staff Giving Publish End Date should be less than or equal to Activation End Date.</description>
    <errorConditionFormula>AND(
    OR(
        ISNEW(),
        ISCHANGED(Staff_Giving_Publish_End_Date__c)
    ),
    NOT(ISBLANK(Staff_Giving_Publish_End_Date__c)),
    Staff_Giving_Publish_End_Date__c &gt; Activation_End_Date__c  
)</errorConditionFormula>
    <errorDisplayField>Staff_Giving_Publish_End_Date__c</errorDisplayField>
    <errorMessage>Staff Giving Publish End Date should be less than or equal to Activation End Date</errorMessage>
</ValidationRule>
