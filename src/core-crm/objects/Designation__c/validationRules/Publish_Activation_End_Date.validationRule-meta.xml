<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Publish_Activation_End_Date</fullName>
    <active>true</active>
    <description>Public Giving Publish End Date should be less than or equal to Activation End Date.</description>
    <errorConditionFormula>AND(
    OR(
        ISNEW(),
        ISCHANGED(Public_Giving_Publish_End_Date__c)
    ),    
    NOT(ISBLANK(Public_Giving_Publish_End_Date__c)),
    Public_Giving_Publish_End_Date__c &gt; Activation_End_Date__c 
)</errorConditionFormula>
    <errorDisplayField>Public_Giving_Publish_End_Date__c</errorDisplayField>
    <errorMessage>Public Giving Publish End Date should be less than or equal to Activation End Date</errorMessage>
</ValidationRule>
