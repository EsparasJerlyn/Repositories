<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Facilitator_is_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 Is_Managed_Externally__c = false,
 ISBLANK( Facilitator__c ) 
)</errorConditionFormula>
    <errorDisplayField>Facilitator__c</errorDisplayField>
    <errorMessage>Facilitator is required</errorMessage>
</ValidationRule>
