<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Time_should_be_greater_than_start</fullName>
    <active>true</active>
    <errorConditionFormula>TIMEVALUE(End_Time_v2__c ) &lt; TIMEVALUE( Start_Time_v2__c )</errorConditionFormula>
    <errorDisplayField>End_Time_v2__c</errorDisplayField>
    <errorMessage>Should be greater than start time</errorMessage>
</ValidationRule>
