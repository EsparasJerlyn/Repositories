<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RT_Publish_Start_Date_End_Date</fullName>
    <active>true</active>
    <description>Publishing Start Date should be less than Publishing End Date.</description>
    <errorConditionFormula>AND(
    OR(
        ISNEW(),
        ISCHANGED(QUT_Giving_Publish_Start_Date__c)
    ),
    NOT(ISBLANK(QUT_Giving_Publish_Start_Date__c)),
    QUT_Giving_Publish_Start_Date__c &gt;=  QUT_Giving_Publish_End_Date__c 
)</errorConditionFormula>
    <errorMessage>Publishing Start Date should be less than Publishing End Date.</errorMessage>
</ValidationRule>
